<Window x:Class="SortedListApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Margin="50,50,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Margin="50,75,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Margin="200,50,0,0" VerticalAlignment="Top" Height="43" Width="100" Click="addBtn_Click"/>

    </Grid>
</Window>
